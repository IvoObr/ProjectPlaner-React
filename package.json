{
  "name": "project-planer-react",
  "version": "0.1.0",
  "private": true,
  "description": "full stack project app",
  "keywords": [
    "MERN",
    "Express",
    "NodeJS",
    "MongoDB",
    "Mongoose",
    "JavaScript",
    "JsonWebToken",
    "ReactJS"
  ],
  "author": "IvoObr",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^9.0.0",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-loop": "^4.5.4",
    "serve": "^10.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!<rootDir>/public/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": ["json", "lcov", "text", "clover"]
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.5.0",
    "redux-mock-store": "^1.5.3",
    "terser": "^3.16.1"
  },
  "heroku-run-build-script": true
}
